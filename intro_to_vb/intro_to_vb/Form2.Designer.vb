    Private Sub RadioButton_CheckedChanged(sender As Object, e As EventArgs) Handles RadioButton1.CheckedChanged, RadioButton2.CheckedChanged, RadioButton3.CheckedChanged
        ' When any radio button is checked, update sample text font
        UpdateSampleTextFont()
    End Sub

    Private Sub UpdateSampleTextFont()
        ' Determine which radio button is checked and set font accordingly
        If RadioButton1.Checked Then
            TextBox1.Font = New Font("Garamond", 12)
        ElseIf RadioButton2.Checked Then
            TextBox1.Font = New Font("Magneto", 12)
        ElseIf RadioButton3.Checked Then
            TextBox1.Font = New Font("Tahoma", 12)
        End If
    End Sub

